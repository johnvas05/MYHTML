<MYHTML>
<head>
<title>Test SRC Validation</title>
</head>
<body>
    <!-- Test absolute URLs -->
    <img src="https://example.com/image.jpg" alt="Example" id="img1">
    <img src="http://test.com/pic.png" alt="Test" id="img2">
    <img src="ftp://files.example.com/doc.pdf" alt="FTP" id="img3">
    <img src="file:///C:/images/local.jpg" alt="Local" id="img4">

    <!-- Test relative URLs -->
    <img src="images/photo.jpg" alt="Photo" id="img5">
    <img src="../documents/image.png" alt="Document" id="img6">
    <img src="./picture.gif" alt="Picture" id="img7">

    <!-- Test invalid URLs -->
    <img src="invalid<image.jpg" alt="Invalid" id="img8">
    <img src="space in name.jpg" alt="Invalid Space" id="img9">
</body>
</MYHTML> 